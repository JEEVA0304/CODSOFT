import random
randomcharacter="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789&@#!$^%"
numpwd=int(input("Enter the number of password to be generated: "))
pwdlen= int(input("Enter the length of the password needed: "))
print("PASSWORD GENERATOR")
for i in range(numpwd):
    password=""
    for j in range(pwdlen):
        password=password + random.choice(randomcharacter)
    print(password)
